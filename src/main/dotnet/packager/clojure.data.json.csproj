<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;netstandard2.1</TargetFrameworks>
  </PropertyGroup>

	
	<PropertyGroup>
		<PackageId>clojure.data.json</PackageId>
		<RootNamespace>clojure.data</RootNamespace>		
		<Title>clojure.data.json</Title>
		<Product>clojure.data.json</Product>
		<AssemblyTitle>clojure.data.json</AssemblyTitle>
		<Authors>ClojureCLR contributos</Authors>
		<Description>A port of the data.json library to ClojureCLR.</Description>
		<Copyright>Copyright Â© Rich Hickey, ClojureCLR contributors 2022</Copyright>
		<PackageLicenseExpression>EPL-1.0</PackageLicenseExpression>
		<RepositoryUrl>https://github.com/clojure/clojure.tools.namesapce</RepositoryUrl>
		<Company>ClojureCLR contributors</Company>
		<PackageTags>Clojure;ClojureCLR</PackageTags>
		<Version>2.4.0</Version> 
	</PropertyGroup>

	<ItemGroup>
		<EmbeddedResource CopyToOutputDirectory="Never" Include ="..\..\clojure\clojure\data\**\*"/>
	</ItemGroup>	
	
</Project>
